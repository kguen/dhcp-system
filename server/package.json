{
  "name": "dhcp-api",
  "version": "1.0.0",
  "engines": {
    "node": ">=11.14.0"
  },
  "main": "index.js",
  "scripts": {
    "start": "sudo node -r dotenv/config index.js",
    "dev": "sudo nodemon -r dotenv/config --ignore seeders/ --ignore migrations/ --ignore scripts/ index.js",
    "ldap-seed": "node -r dotenv/config scripts/seedLDAP.js",
    "ldap-purge": "node -r dotenv/config scripts/purgeLDAP.js",
    "dhcp-config": "sudo node scripts/configDHCP.js",
    "db-setup": "npx sequelize db:migrate && npx sequelize db:seed:all && yarn run ldap-seed && yarn run dhcp-config",
    "db-purge": "npx sequelize db:migrate:undo:all && yarn run ldap-purge"
  },
  "author": "kguen",
  "license": "ISC",
  "devDependencies": {
    "eslint": "^7.2.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "sequelize-cli": "^6.2.0"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "is-cidr": "^4.0.2",
    "jsonwebtoken": "^8.5.1",
    "ldapjs": "^2.2.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "mysql2": "^2.2.5",
    "netmask": "^2.0.1",
    "sequelize": "^6.5.0"
  }
}
